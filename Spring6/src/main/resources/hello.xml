<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"

	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"

	xmlns:context="http://www.springframework.org/schema/context"

	xsi:schemaLocation="http://www.springframework.org/schema/beans

   http://www.springframework.org/schema/beans/spring-beans.xsd

   http://www.springframework.org/schema/context

   http://www.springframework.org/schema/context/spring-context.xsd"
	default-init-method="customInit" default-destroy-method="customDestroy">

	<bean id="stu" class="com.pack.Student">
		<property name="sid" value="2000" />
		<property name="name" value="Bhargava" />

	</bean>
	<bean id="ex1" class="com.pack.Example">
		<property name="message" value="Hello world" />


	</bean>

	<bean id="course" class="com.pack.Course">
		<property name="cid" value="102" />
		<property name="cname" value="Pinky" />
	</bean>

</beans>